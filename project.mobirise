          "_name": "mobigarymenu1",
          "_sourceTheme": "mobirise5",
          "_customHTML": "<section class=\"menu\" group=\"Menu\" plugins=\"DropDown, TouchSwipe\" always-top global once=\"menu\" not-draggable position-absolute>\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n                \n        <header>Global Menu Config</header>\n        <input type=\"range\" inline title=\"Menu Top Line Font Weight\" name=\"MenuTLFontWeight\" min=\"100\" max=\"900\" step=\"100\" value=\"400\">\n        <input type=\"range\" inline title=\"Menu Font Weight\" name=\"MenuFontWeight\" min=\"100\" max=\"900\" step=\"100\" value=\"400\">\n        <input type=\"range\" inline title=\"MM Minumum Height\" name=\"MMMinHeight\" min=\"40\" max=\"150\" step=\"1\" value=\"68\"> \n        \n        <header>Menu Top Line (TL) Config</header>\n        <input type=\"checkbox\" title=\"Show TL\" name=\"showTopLine\">\n        <input type=\"checkbox\" title=\"TL on HamMenu\" name=\"showTopLineHamMenu\" condition=\"showTopLine\" checked>\n        <input type=\"checkbox\" title=\"TL on Scroll Sticky\" name=\"showTopLineOnScroll\" condition=\"showTopLine\">\n        <select title=\"TL Effect\" name=\"TopLineEffect\" condition=\"showTopLine\">\n            <option value=\"TopLineEffectNone\" selected>None</option>\n            <option value=\"TopLineEffectPulse\">Pulse</option>\n            <option value=\"TopLineEffectFlash\">Flash</option>\n            <option value=\"TopLineEffectScroll\">Scroll</option>\n        </select>\n        <input type=\"range\" inline title=\"TL Effect Pulse Speed\" name=\"TopLineEffectPulseSpeed\" min=\"0.5\" max=\"5\" step=\"0.1\" value=\"2.7\" condition=\"TopLineEffect=='TopLineEffectPulse' && showTopLine\">\n        <input type=\"range\" inline title=\"TL Effect Pulse Count\" name=\"TopLineEffectPulseCount\" min=\"1\" max=\"50\" step=\"1\" value=\"5\" condition=\"TopLineEffect=='TopLineEffectPulse' && showTopLine\">\n        <input type=\"range\" inline title=\"TL Effect Scroll Speed\" name=\"TopLineEffectScrollSpeed\" min=\"5\" max=\"100\" step=\"1\" value=\"15\" condition=\"TopLineEffect=='TopLineEffectScroll' && showTopLine\">\n        <input type=\"range\" inline title=\"TL Effect Scroll Window\" name=\"TopLineEffectScrollWindow\" min=\"50\" max=\"100\" step=\"0.5\" value=\"100\" condition=\"TopLineEffect=='TopLineEffectScroll' && showTopLine\">\n        <input type=\"range\" inline title=\"TL Effect Scroll Time Gap\" name=\"TopLineEffectScrollTimeGap\" min=\"1\" max=\"2\" step=\"0.1\" value=\"1\" condition=\"TopLineEffect=='TopLineEffectScroll' && showTopLine\">\n        <input type=\"range\" inline title=\"TL Effect Flash Speed\" name=\"TopLineEffectFlashSpeed\" min=\"0.5\" max=\"5\" step=\"0.1\" value=\"2.6\" condition=\"TopLineEffect=='TopLineEffectFlash' && showTopLine\">\n        <input type=\"range\" inline title=\"TL Effect Flash Count\" name=\"TopLineEffectFlashCount\" min=\"1\" max=\"50\" step=\"1\" value=\"5\" condition=\"TopLineEffect=='TopLineEffectFlash' && showTopLine\">\n        <input type=\"checkbox\" title=\"TL Scroll Mouse Hover Pause\" name=\"TopLineMouseHoverPause\" condition=\"TopLineEffect=='TopLineEffectScroll' && showTopLine\" checked>\n        <input type=\"range\" inline title=\"TL BG Opacity\" name=\"menuTopLineBgOpacity\" min=\"0\" max=\"1\" step=\"0.05\" value=\"1\" condition=\"showTopLine\">\n        <input type=\"color\" title=\"TL BG Color\" name=\"menuTopLineBgColor\" value=\"#0684ff\" condition=\"showTopLine\">\n        <input type=\"checkbox\" title=\"TL BG Gradient Color\" name=\"menuTopLineBgGradientCol\" condition=\"showTopLine\" checked>\n        <select title=\"TL BG Gradient Color Style\" name=\"menuTopLineGradientBgStyle\" condition=\"menuTopLineBgGradientCol && showTopLine\">\n            <option value=\"menuTopLineGradientBgLR2\">L-R 2 col</option>\n            <option value=\"menuTopLineGradientBgLR3\">L-R 3 Col</option>\n            <option value=\"menuTopLineGradientBgLR4\" selected>L-R 4 Col</option>\n            <option value=\"menuTopLineGradientBgTB2\">T-B 2 Col</option>\n            <option value=\"menuTopLineGradientBgTB3\">T-B 3 Col</option>\n            <option value=\"menuTopLineGradientBgTB4\">T-B 4 Col</option>\n        </select>\n        <input type=\"color\" title=\"TL BG Color 2\" name=\"menuTopLineBgColor2\" value=\"#04a5fa\" condition=\"menuTopLineGradientBgStyle=='menuTopLineGradientBgLR2' && menuTopLineBgGradientCol && showTopLine || menuTopLineGradientBgStyle=='menuTopLineGradientBgLR3' && menuTopLineBgGradientCol && showTopLine || menuTopLineGradientBgStyle=='menuTopLineGradientBgLR4' && menuTopLineBgGradientCol && showTopLine || menuTopLineGradientBgStyle=='menuTopLineGradientBgTB2' && menuTopLineBgGradientCol && showTopLine || menuTopLineGradientBgStyle=='menuTopLineGradientBgTB3' && menuTopLineBgGradientCol && showTopLine  || menuTopLineGradientBgStyle=='menuTopLineGradientBgTB4'&&  menuTopLineBgGradientCol && showTopLine\">\n        <input type=\"color\" title=\"TL BG Color 3\" name=\"menuTopLineBgColor3\" value=\"#47b5ed\" condition=\"menuTopLineGradientBgStyle=='menuTopLineGradientBgLR3' && menuTopLineBgGradientCol && showTopLine || menuTopLineGradientBgStyle=='menuTopLineGradientBgLR4' && menuTopLineBgGradientCol && showTopLine || menuTopLineGradientBgStyle=='menuTopLineGradientBgTB3' && menuTopLineBgGradientCol && showTopLine || menuTopLineGradientBgStyle=='menuTopLineGradientBgTB4' &&  menuTopLineBgGradientCol && showTopLine\">\n        <input type=\"color\" title=\"TL BG Color 4\" name=\"menuTopLineBgColor4\" value=\"#ffffff\" condition=\"menuTopLineGradientBgStyle=='menuTopLineGradientBgLR4' && menuTopLineBgGradientCol && showTopLine || menuTopLineGradientBgStyle=='menuTopLineGradientBgTB4' && menuTopLineBgGradientCol && showTopLine\">\n        <input type=\"color\" title=\"TL Top Border Color\" name=\"TopLineULTopCol\" value=\"#232323\" condition=\"showTopLine\">\n        <input type=\"range\" inline title=\"TL Top Border Thi